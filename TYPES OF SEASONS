/*******************************************************RAINY SEASON**************************************/
#include<stdio.h>
#include<GL/glut.h>
float x;
float y;
int i=0;
void timersss(int v)
{
x=x+0.01;
if(i<200)
{
y=y+0.05;
i++;
}
else{
    i=0;
}
glutPostRedisplay();
glutTimerFunc(9000000000000000000000000000,timersss,0);
}
void third()
{
//road
 glClear(GL_COLOR_BUFFER_BIT);
 glColor3f(0.0,0.0,0.0);
 glBegin(GL_POLYGON);
 glVertex2f(0.0,0.0);
 glVertex2f(0.0,30.0);
 glVertex2f(100.0,30.0);
 glVertex2f(100.0,0.0);
 glEnd();

//white strip 1
