}
void first()
{
 glClear(GL_COLOR_BUFFER_BIT);
 circle();
 /*glPushMatrix();
 glTranslatef(20.0,30.0,0.0);
 glScalef(0.1,0.1,0.0);
 glPopMatrix();*/
 glPushMatrix();
 glRasterPos3f(14.0,90.0,0.0);
 glColor3f(0.0,0.7,0.0);
 output("MANGALORE INSTITUTE OF TECHNOLOGY AND ENGINEERING");
 glRasterPos3f(15.0,80.0,0.0);
 glColor3f(0.0,0.0,0.7);
 output("DEPARTMENT OF COMPUTER SCIENCE AND ENGINEERING");
 glRasterPos3f(35.0,70.0,0.0);
 glColor3f(0.2,0.1,0.4);
 output("DIFFERENT TYPES OF SEASON");
 glRasterPos3f(5.0,40.0,0.0);
 glColor3f(0.2,0.1,0.4);
 output("VINEETH KM");
 glRasterPos3f(12.0,36.0,0.0);
 glColor3f(0.2,0.1,0.4);
 output("4MT19CS177");
 glRasterPos3f(75.0,40.0,0.0);
 glColor3f(0.2,0.5,0.0);
 output("VEENA NAYAK");
 glRasterPos3f(79.0,36.0,0.0);
 glColor3f(0.2,0.5,0.0);
 output("4MT19CS175");
 glRasterPos3f(40.0,20.0,0.0);
 glColor3f(0.2,0.1,0.4);
 output("Under The Guidance of");
 glRasterPos3f(44.0,15.0,0.0);
 glColor3f(0.2,0.1,0.4);
 output("Ms AISHWARYA M BHAT");
 glPopMatrix();
}
/**************************************************************SUMMER SEASON**************************************/
float g;
float b;
int h=0;
void timerss(int d)
{
g=g+0.01;
if(h<200)
{
b=b+0.05;
h++;
}
else{
    h=0;
}
glutPostRedisplay();
glutTimerFunc(900000000000000000,timerss,0);
}
