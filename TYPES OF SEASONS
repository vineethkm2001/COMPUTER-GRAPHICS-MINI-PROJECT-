 glVertex2f(20.5,40.0);
 glVertex2f(21.0,40.0);
 glVertex2f(21.0,30.0);
 glEnd();
 glFlush();
  //bus stop circle1
 glColor3f(0.0,0.0,0.0);
glMatrixMode(GL_MODELVIEW);
