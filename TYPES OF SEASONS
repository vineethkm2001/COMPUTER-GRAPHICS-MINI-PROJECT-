static float r1= 0;
static float r2= 10;
static float r3= 20;
static float r4= 30;
static float r5= 40;
static float r6= 50;
static float r7= 60;
static float r8= 70;
static float r9= 80;
static float r10= 90;
static float r11= 100;
static float r12= 110;

void rainfall()
{
    r1 -= 3;
    if(r1 < -100)
        r1 = 0;
    glutPostRedisplay();

    r2 -= 3;
    if(r2 < -100)
        r2 = 0;
    glutPostRedisplay();

    r3 -= 0.5;
    if(r3 < -100)
        r3 = 0;
    glutPostRedisplay();

    r4 -= 3;
    if(r4 < -100)
        r4 = 0;
    glutPostRedisplay();

    r5 -= 3;
    if(r5 < -100)
        r5 = 0;
    glutPostRedisplay();

    r6 -= 3;
    if(r6 < -100)
        r6 = 0;
    glutPostRedisplay();

    r7 -= 3;
    if(r7 < -100)
        r7 = 0;
    glutPostRedisplay();

    r8 -= 3;
    if(r8 < -100)
        r8 = 0;
    glutPostRedisplay();

    r9 -= 3;
    if(r9 < -100)
        r9 = 0;
    glutPostRedisplay();

    r10 -= 3;
    if(r10 < -100)
        r10 = 0;
    glutPostRedisplay();
    r11 -= 3;
    if(r11 < -100)
        r11 = 0;
    glutPostRedisplay();

    r12 -= 3;
    if(r12 < -100)
        r12 = 0;
    glutPostRedisplay();
}

void Rain()
{
    rainfall();
    glColor3f(1,1,1);
    int y = 97;
    for(int i = 0;i<100;i= i+2){
        glBegin(GL_LINES);
        glLineWidth(20);
        glVertex2d(i,y);
        glVertex2d(i-0.5,y-1);
    glEnd();
//for(i3=0;i3<100000;i3++);
glFlush();
    }
}

void rainFall()
{
    glPushMatrix();
        glTranslated(0,r1,0);
        Rain();
    glPopMatrix();

    glPushMatrix();
        glTranslated(0,r2,0);
        Rain();
    glPopMatrix();

    glPushMatrix();
        glTranslated(0,r3,0);
        Rain();
    glPopMatrix();

    glPushMatrix();
        glTranslated(0,r4,0);
        Rain();
    glPopMatrix();

    glPushMatrix();
        glTranslated(0,r5,0);
        Rain();
    glPopMatrix();

    glPushMatrix();
        glTranslated(0,r6,0);
        Rain();
    glPopMatrix();

    glPushMatrix();
        glTranslated(0,r7,0);
        Rain();
    glPopMatrix();

    glPushMatrix();
        glTranslated(0,r8,0);
        Rain();
    glPopMatrix();

    glPushMatrix();
        glTranslated(0,r9,0);
        Rain();
    glPopMatrix();

    glPushMatrix();
        glTranslated(0,r10,0);
        Rain();
    glPopMatrix();

    glPushMatrix();
        glTranslated(0,r11,0);
        Rain();
    glPopMatrix();
    //glutSwapBuffers();


}
/*********************************************************WINTER SEASON *****************************************/

void fourth()
{
//road
 glClear(GL_COLOR_BUFFER_BIT);
 glColor3f(0.0,0.0,0.0);
 glBegin(GL_POLYGON);
 glVertex2f(0.0,0.0);
 glVertex2f(0.0,30.0);
 glVertex2f(100.0,30.0);
 glVertex2f(100.0,0.0);
 glEnd();

//white strip 1
 glColor3f(1.0,1.0,1.0);
 glBegin(GL_POLYGON);
