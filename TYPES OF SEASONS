 glVertex2f(42.0,15.0);
 glVertex2f(67.0,15.0);
 glVertex2f(65.0,10.0);
 glEnd();
//white strip 3
 glColor3f(1.0,1.0,1.0);
 glBegin(GL_POLYGON);
 glVertex2f(80.0,10.0);
 glVertex2f(82.0,15.0);
 glVertex2f(97.0,15.0);
 glVertex2f(95.0,10.0);
 glEnd();
 glFlush();

  //footpath
 glColor3f(1,0.8,0.6);
 glBegin(GL_POLYGON);
 glVertex2f(0.0,25.0);
 glVertex2f(0.0,30.0);
 glVertex2f(100.0,30.0);
 glVertex2f(100.0,25.0);
 glEnd();
 //footpath lines
