#include<stdio.h>
#include<GL/glut.h>
int slide=0;
/***********************************************************SLIDE INTRO**********************************************/
float x1;
float y1;
int i1=0;
int i3;
void timer(int v1)
{
x1=x1+10;
if(i1<56)
{
y1=y1+0.5;
i1++;
}
glutPostRedisplay();
glutTimerFunc(100,timer,10);
}
void output(char *s)
{
 int k;
 for(k=0;k<strlen(s);k++)
 glutBitmapCharacter(GLUT_BITMAP_TIMES_ROMAN_24,s[k]);
}
void circle()
{
glColor3f(1,0.6,0.1);
glMatrixMode(GL_MODELVIEW);
glPushMatrix();
glTranslatef(47.5,50.5,0.0);
glScalef(0.2,0.3,1);
glutSolidSphere(15,100,50);
glPopMatrix();
//cloud1
glColor3f(1.0,1.0,1.0);
glMatrixMode(GL_MODELVIEW);
glPushMatrix();
glTranslatef(20.0+y1,45.5,0.0);
glScalef(0.2,0.3,1);
glutSolidSphere(15,100,50);
glPopMatrix();
//CLOUD2
glColor3f(1.0,1.0,1.0);
glMatrixMode(GL_MODELVIEW);
glPushMatrix();
glTranslatef(23.5+y1,46.5,0.0);
glScalef(0.2,0.3,1);
glutSolidSphere(15,100,50);
glPopMatrix();
//CLOUD3
glColor3f(1.0,1.0,1.0);
glMatrixMode(GL_MODELVIEW);
glPushMatrix();
glTranslatef(27.5+y1,45.5,0.0);
glScalef(0.2,0.3,1);
glutSolidSphere(15,100,50);
glPopMatrix();
//CLOUD4
glColor3f(1.0,1.0,1.0);
glMatrixMode(GL_MODELVIEW);
glPushMatrix();
glTranslatef(23.5+y1,42.5,0.0);
glScalef(0.2,0.3,1);
glutSolidSphere(15,100,50);
glPopMatrix();
//cloud5
glColor3f(1.0,1.0,1.0);
glMatrixMode(GL_MODELVIEW);
glPushMatrix();
glTranslatef(67.0-y1,45.5,0.0);
glScalef(0.2,0.3,1);
glutSolidSphere(15,100,50);
glPopMatrix();
//CLOUD6

glColor3f(1.0,1.0,1.0);
