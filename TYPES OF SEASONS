void endd()
{

    glBegin(GL_POLYGON);
glColor3f(0.5,1.0,0.1);
glVertex2f(0.0,0.0);
glColor3f(0.6,0.2,0.4);
glVertex2f(0.0,100.0);
glColor3f(0.6,0.2,1.0);
glVertex2f(100.0,100.0);
glColor3f(0.6,0.7,0.9);
glVertex2f(0.0,100.0);
glEnd();
glFlush();
}
void endslide()
{
    glClear(GL_COLOR_BUFFER_BIT);
    endd();
glColor3f(0.0,0.0,0.0);
    glPushMatrix();
glRasterPos3f(44.0,45.0,0.0);
outputsssss("THE END");
glPopMatrix();
glColor3f(0.0,0.0,0.0);
    glPushMatrix();
glRasterPos3f(42.0,35.0,0.0);
outputsssss("THANK YOU");
glPopMatrix();
}

void key(unsigned char key,int x,int y)
{
if (key=='2'||key=='2')
    {
    slide=1;
    glutPostRedisplay();
    }
    else if (key=='4'||key=='4')
    {
    slide=2;
    glutPostRedisplay();
    }
     else if (key=='1'||key=='1')
    {
    slide=3;
    glutPostRedisplay();
    }
     else if (key=='3'||key=='3')
    {
    slide=4;
    glutPostRedisplay();
    }
    else if (key=='5'||key=='5')
    {
    slide=5;
    glutPostRedisplay();
    }
    else if (key=='6'||key=='6')
    {
    slide=6;
    glutPostRedisplay();
    }
    else if (key=='7'||key=='7')
    {
    slide=7;
    glutPostRedisplay();
    }
    else if (key=='e'||key=='E')
    {
    exit(0);
    }
}
void display()
    {
    glClear(GL_COLOR_BUFFER_BIT);
    glClearColor(0.3,1,1,0.0);
    first();
   //timer(0);
  if(slide==1)
{

second();
timerss(0);
glutPostRedisplay();
}
 else if(slide==2)
{
   third();
timersss(0);
rainFall();
glutPostRedisplay();
}
else if(slide==3)
{
    glClearColor(1.0,1.0,1.0,0.0);
summer();
glutPostRedisplay();
}
else if(slide==4)
{
    glClearColor(1.0,1.0,1.0,0.0);
rainy();
glutPostRedisplay();
}
else if(slide==5)
{
    glClearColor(1.0,1.0,1.0,0.0);
snowfall();
glutPostRedisplay();
}
else if(slide==6)
{
glClearColor(0.0,0.0,0.0,0.0);
    fourth();
    timersss(0);
Fall();
//glutPostRedisplay();
}
else if(slide==7)
{
    glClearColor(1.0,1.0,1.0,0.0);
endslide();
glutPostRedisplay();
}
glutSwapBuffers();

glFlush();
    }

int main(int argc,char **argv)
{
 glutInit(&argc,argv);
 glutInitDisplayMode(GLUT_DOUBLE|GLUT_RGB);
 glutInitWindowSize(1100,700);
 glutInitWindowPosition(100,100);
 glutCreateWindow("CG MINI PROJECT");
 glClearColor(0.3,1,1,0.0);
 glMatrixMode(GL_PROJECTION);
 glLoadIdentity();
 glOrtho(0.0,100.0,0.0,100.0,0.0,100.0);
 glutDisplayFunc(display);
 glutKeyboardFunc(key);
 timer(0);
timersss(0);
 timerss(0);
 glutMainLoop();
 return 0;

}
