static float s1= 0;
static float s2= 10;
static float s3= 20;
static float s4= 30;
static float s5= 40;
static float s6= 50;
static float s7= 60;
static float s8= 70;
static float s9= 80;
static float s10= 90;
static float s11= 100;
static float s12= 110;
static float s13= 120;
static float s14= 130;

void srainfall()
{
    s1 -= 3;
    if(s1< -100)
        s1 = 0;
   glutPostRedisplay();

    s2 -= 3;
    if(s2 < -100)
        s2 = 0;
    glutPostRedisplay();

    s3 -= 3;
    if(s3< -100)
        s3= 0;
    glutPostRedisplay();

    s4 -= 3;
    if(s4 < -100)
        s4 = 0;
    glutPostRedisplay();

    s5 -= 3;
    if(s5 < -100)
        s5 = 0;
    glutPostRedisplay();

    s6 -= 3;
    if(s6 < -100)
        s6 = 0;
    glutPostRedisplay();

    s7 -= 3;
    if(s7 < -100)
        s7 = 0;
    glutPostRedisplay();

    s8 -= 3;
    if(s8< -100)
        s8 = 0;
    glutPostRedisplay();

    s9 -= 3;
    if(s9 < -100)
        s9 = 0;
    glutPostRedisplay();

    s10 -= 3;
    if(s10 < -100)
        s10 = 0;
    glutPostRedisplay();
    s11 -= 3;
    if(s11 < -100)
        s11 = 0;
    glutPostRedisplay();

    s12 -= 3;
    if(s12 < -100)
        s12 = 0;
    glutPostRedisplay();

     s13 -= 3;
    if(s13 < -100)
        s13 = 0;
    glutPostRedisplay();

     s13 -= 3;
    if(s13 < -100)
        s13 = 0;
    glutPostRedisplay();
}

void SRain()
{
    srainfall();
    glColor3f(1,1,1);
    int y2= 97;
    for(float i2 = 0.0;i2<100.0;i2= i2+2.0){
             glPointSize(3.0);
        glBegin(GL_POINTS);

        glVertex2d(i2,y2);
        glVertex2d(i2,y2-1);
    glEnd();
    for(i3=0;i3<10000;i3++);
    glFlush();

    }
}

void Fall()
{
    glPushMatrix();
        glTranslated(0,s1,0);
        SRain();
    glPopMatrix();

    glPushMatrix();
        glTranslated(0,s2,0);
        SRain();
    glPopMatrix();

    glPushMatrix();
        glTranslated(0,s3,0);
        SRain();
    glPopMatrix();

    glPushMatrix();
        glTranslated(0,s4,0);
        SRain();
    glPopMatrix();

    glPushMatrix();
        glTranslated(0,s5,0);
        SRain();
    glPopMatrix();

    glPushMatrix();
        glTranslated(0,s6,0);
        SRain();
    glPopMatrix();

    glPushMatrix();
        glTranslated(0,s7,0);
        SRain();
    glPopMatrix();

    glPushMatrix();
        glTranslated(0,s8,0);
        SRain();
    glPopMatrix();

    glPushMatrix();
        glTranslated(0,s9,0);
        SRain();
    glPopMatrix();

    glPushMatrix();
        glTranslated(0,s10,0);
        SRain();
    glPopMatrix();

    glPushMatrix();
        glTranslated(0,s11,0);
        SRain();
    glPopMatrix();

    glPushMatrix();
        glTranslated(0,s12,0);
        SRain();
    glPopMatrix();

    glPushMatrix();
        glTranslated(0,s13,0);
        SRain();
    glPopMatrix();

    glPushMatrix();
        glTranslated(0,s14,0);
        SRain();
    glPopMatrix();
}
/***********************************************SUMMER SEASON DISPLAY******************/
int z;
void outputss(char *s2)
{
int k2;
for(k2=0;k2<strlen(s2);k2++)
glutBitmapCharacter(GLUT_BITMAP_TIMES_ROMAN_24,s2[k2]);
}
void sixth()
{
glBegin(GL_POLYGON);
glColor3f(0.5,1.0,0.1);
glVertex2f(0.0,0.0);
glColor3f(0.6,0.2,0.4);
glVertex2f(0.0,100.0);
glColor3f(0.6,0.2,1.0);
glVertex2f(100.0,100.0);
glColor3f(0.6,0.7,0.9);
glVertex2f(0.0,100.0);
glEnd();
glFlush();
}
void summer()
{
    glClear(GL_COLOR_BUFFER_BIT);
    sixth();
glColor3f(0.0,0.0,0.0);
    glPushMatrix();
glRasterPos3f(40.0,45.0,0.0);
outputss("SUMMER SEASON");
glPopMatrix();
glColor3f(0.0,0.0,0.0);
    glPushMatrix();
glRasterPos3f(30.0,35.0,0.0);
outputss("SUMMER IS THE HOTTEST OF THE FOUR TEMPERATE SEAONS ");
glPopMatrix();
glPushMatrix();
glRasterPos3f(30.0,30.0,0.0);
outputss("OCCURING AFTER SPRING AND BEFORE AUTUMN ");
glPopMatrix();
}
